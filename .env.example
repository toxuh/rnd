# ESP32 Random Server Configuration
RND_SERVER_URL=http://192.168.1.1

# Redis Configuration (for rate limiting and caching)
REDIS_URL=redis://localhost:6379

# API Key Authentication
API_KEY_MAIN=rnd_your_main_api_key_here
API_KEY_ADMIN=rnd_your_admin_api_key_here
API_KEY_LIMITED=rnd_your_limited_api_key_here

# Security Configuration
DISABLE_API_KEY_AUTH=false
ALLOWED_ORIGINS=http://localhost:3000,https://yourdomain.com
WEBHOOK_SECRET=your_webhook_secret_here

# Rate Limiting Configuration (requests per minute)
RATE_LIMIT_GLOBAL_MAX=100
RATE_LIMIT_RANDOM_MAX=30
RATE_LIMIT_STRICT_MAX=10

# Additional Security
MAX_REQUEST_SIZE=10240
ENABLE_REQUEST_LOGGING=true

# Client-side API Key (for frontend requests)
NEXT_PUBLIC_API_KEY=rnd_your_public_api_key_here
